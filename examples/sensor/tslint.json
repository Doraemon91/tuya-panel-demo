{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-config-standard",
      "tslint-react",
      "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "member-access": false,
      "jsx-no-lambda": false,
      "no-empty": false,
      "no-console": false,
      "prettier": true,
      "interface-name": false,
      "no-bitwise": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
